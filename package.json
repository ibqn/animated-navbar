{
  "name": "animated-navbar",
  "version": "1.0.0",
  "description": "animated top menu",
  "repository": "ibqn/animated-navbar",
  "main": "index.html",
  "scripts": {
    "start": "concurrently --names \"SASS,WATCH,SERVE\" --prefix-colors \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run sass\" \"npm run sass:watch\" \"npm run serve\"",
    "serve": "lite-server",
    "sass": "node-sass style.sass --output ./ --indent-width 2 --output-style expanded --indent-type space --source-map true",
    "sass:watch": "npm run sass -- --watch"
  },
  "author": "ibqn",
  "license": "ISC",
  "dependencies": {
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.13.1"
  }
}
